{
  "name": "@chromatika/tailwind-plugin",
  "version": "0.1.3",
  "description": "Tailwind plugin for Chromatika color scales",
  "author": "Nicole Hollant <nicolehollant@gmail.com>",
  "homepage": "https://github.com/tkofh/chromatika#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/chromatika.git"
  },
  "scripts": {
    "build": "unbuild",
    "prepublish": "yarn build",
    "playground": "vite ./playground/"
  },
  "bugs": {
    "url": "https://github.com/tkofh/chromatika/issues"
  },
  "devDependencies": {
    "tailwindcss": "^3.2.7",
    "unbuild": "^1.1.1",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "@chromatika/core": "0.1.3"
  },
  "gitHead": "e7df4a03b4d81d3189518b6ae71ee0536968e183"
